void setup()
{
  pinMode(2, INPUT);  // Set pin 2 as an input
   //setup LEDs
  delay(2000);
  LEDS.addLeds<LED_TYPE, LED_PIN, COLOR_ORDER>(leds, NUM_LEDS).setCorrection(TypicalLEDStrip);
  FastLED.setBrightness(MAX_BRIGHTNESS);

  //load frames from the animation.h file
  matrix.loadSequence(frames);
  // start the matrix
  matrix.begin();
  //play the animation on the matrix
  matrix.play(true);
  //define LED_BUILTIN as an output
  pinMode(LED_BUILTIN, OUTPUT);

  randomSeed(micros());
  
}

void loop()
{
  int pinState = digitalRead(2);  // Read the state of pin 2

  oled.begin();
  // clear(ALL) will clear out the OLED's graphic memory.
  // clear(PAGE) will clear the Arduino's display buffer.
  oled.clear(ALL);  // Clear the display's memory (gets rid of artifacts)
  // To actually draw anything on the display, you must call the
  // display() function. 
  oled.display();   
  delay(1000);//pause for the splash screen
  //Note: You can change the spalsh screen by editing the array founf in the library source code
  
  oled.clear(PAGE);//clear the screen before we draw our image
  //home
   if (pinState == LOW) {
    
      // home
      oled.drawBitmap(Home);  // Call the drawBitmap function and pass it the array from above
      oled.display();  // Display the image
      delay(2000);
      // W
      oled.drawBitmap(West); 
      oled.display();  // Display the image
      delay(2000);
      // sus
      oled.drawBitmap(sus); 
      oled.display();  // Display the image
      delay(2000);
      // East
      oled.drawBitmap(East); 
      oled.display();  // Display the image
      delay(2000);  // Wait for a moment
   }
   else if (pinState == HIGH) {
    // If pin 2 is HIGH, display "sus" bitmap
    oled.drawBitmap(sus); 
    oled.display();  // Display the image
    delay(2000);
    oled.drawBitmap(Home);  // Call the drawBitmap function and pass it the array from above
      oled.display();  // Display the image
      delay(2000);
  }

}

  // Blue transition mode function
  void BlueTransitionMode(){
  static uint8_t brightness = 0;
  static int8_t direction = 5;

  // Update the brightness
  brightness += direction;
  
  // Reverse the direction at the limits
  if (brightness == 0 || brightness == 255) {
    direction = -direction;
  }

  // Set all LEDs to the blue color with the current brightness
  fill_solid(leds, NUM_LEDS, CRGB(0, 0, brightness));
  
  // Send the data to the LEDs to update the colors
  FastLED.show();
  
  // Wait to create the pulsing effect
  delay(30);
}

  //night light mode
  void RedTransitionMode(){
  static uint8_t brightness = 0;
  static int8_t direction = 5;

  // Update the brightness
  brightness += direction;
  
  // Reverse the direction at the limits
  if (brightness == 0 || brightness == 255) {
    direction = -direction;
  }

  // Set all LEDs to the red color with the current brightness
  fill_solid(leds, NUM_LEDS, CRGB(brightness, 0, 0));
  
  // Send the data to the LEDs to update the colors
  FastLED.show();
  
  // Wait to create the pulsing effect
  delay(30);
  }

	